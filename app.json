{
    "name": "summer_reading2",
    "description": "Traverse Area District Library Summer Reading App",
    "website": "https://www.tadl.org/",
    "require": {
        "plugins": [
            {
                "name": "redis",
                "commands": ["redis:create $APP", "redis:link $APP $APP"]
            }, {
                "name": "postgres",
                "commands": ["postgres:create $APP", "postgres:link $APP $APP"]
            }
        ]
    }
}
